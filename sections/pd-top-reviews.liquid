<section id="pd-top-reviews">
  <div class="page-width">
    <div
      data-oke-reviews-carousel
      data-oke-reviews-carousel-link="{{ section.settings.link }}"
      data-oke-reviews-carousel-heading="{{ section.settings.heading }}"
    ></div>
    <img
      class="pd-review-decorator"
      src="data:image/png;base64,"
    >
  </div>
</section>

{% schema %}
{
  "name": "Top Reviews",
  "disabled_on": {
    "groups": ["header", "footer"]
  },
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading"
    },
    {
      "type": "text",
      "id": "link",
      "label": "Link"
    }
  ],
  "presets": [
    {
      "name": "Top Reviews",
      "category": "Product"
    }
  ]
}
{% endschema %}
